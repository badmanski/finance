.panel.panel-default
  .panel-heading
    %h3.panel-title= t(:monthly_summary)
  .panel-body
    %table.table.table-normal
      %tr
        %td.bold= t(:income, count: 2)
        %td= @entries[:total_incomes]
      %tr
        %td.bold= t(:expense, count: 2)
        %td= @entries[:total_expenses]
      %tr
        %td.bold= t(:total_balance)
        %td= @entries[:balance]
